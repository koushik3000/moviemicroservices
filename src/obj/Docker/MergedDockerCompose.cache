name: dockercompose14797702519186939336
services:
  ShowDetailsDb:
    container_name: ShowDetailsDb
    environment:
      POSTGRES_DB: ShowDetailsDb
      POSTGRES_PASSWORD: admin1234
      POSTGRES_USER: admin
    image: postgres
    networks:
      default: null
    ports:
    - mode: ingress
      target: 5432
      published: "5432"
      protocol: tcp
    restart: always
    volumes:
    - type: volume
      source: postgres_data
      target: /var/lib/postgresql/data
      volume: {}
  pgadmin:
    container_name: pgadmin
    environment:
      PGADMIN_DEFAULT_EMAIL: admin@aspnetrun.com
      PGADMIN_DEFAULT_PASSWORD: admin1234
    image: dpage/pgadmin4
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: "5050"
      protocol: tcp
    restart: always
    volumes:
    - type: volume
      source: pgadmin_data
      target: /root/.pgadmin
      volume: {}
  showdetails.api:
    build:
      context: C:\Users\Anil\Source\Repos\moviemicroservices\src
      dockerfile: Services/ShowDetails/ShowDetails.API/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: showdetailsapi
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
networks:
  default:
    name: dockercompose14797702519186939336_default
volumes:
  pgadmin_data:
    name: dockercompose14797702519186939336_pgadmin_data
  postgres_data:
    name: dockercompose14797702519186939336_postgres_data