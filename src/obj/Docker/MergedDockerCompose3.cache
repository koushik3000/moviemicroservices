name: dockercompose4238872244553067560
services:
  booking.api:
    build:
      context: C:\Users\phani\Source\Repos\moviemicroservices\src
      dockerfile: Services/Booking/Booking.API/Dockerfile
    depends_on:
      bookingdb:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ConnectionStrings:SqlServerConnstr: ' Server=bookingdb;Database=BookingDb; User
        Id= SA ;Password= SwN12345678'
    image: bookingapi
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: "2005"
      protocol: tcp
  bookingdb:
    container_name: bookingdb
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_SA_PASSWORD: SwN12345678
    image: mcr.microsoft.com/mssql/server:2017-latest
    networks:
      default: null
    ports:
    - mode: ingress
      target: 1433
      published: "1433"
      protocol: tcp
    restart: always
  paymentdb:
    container_name: paymentdb
    image: redis:alpine
    networks:
      default: null
    ports:
    - mode: ingress
      target: 6379
      published: "6379"
      protocol: tcp
    restart: always
networks:
  default:
    name: dockercompose4238872244553067560_default